<Window x:Class="WPFGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="REGISTER" MinWidth="100" Click="Button_Click" />
            <Button Content="LOGIN" MinWidth="100" Margin="10,0,0,0" Click="Button_Click_1"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />
        <Label x:Name ="user" Content="Label" HorizontalAlignment="Left" Margin="255,4,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
